{
    "name": "venturedrake/laravel-encryptable",
    "description": "Automatically encrypt and decrypt database table fields",
    "keywords": [
        "venturedrake",
        "laravel-encryptable"
    ],
    "homepage": "https://github.com/venturedrake/laravel-encryptable",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Drake",
            "email": "andrew@venturedrake.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Venturedrake\\LaravelEncryptable\\": "src",
            "Venturedrake\\LaravelEncryptable\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Venturedrake\\LaravelEncryptable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Venturedrake\\LaravelEncryptable\\LaravelEncryptableServiceProvider"
            ],
            "aliases": {
                "LaravelEncryptable": "Venturedrake\\LaravelEncryptable\\LaravelEncryptableFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
